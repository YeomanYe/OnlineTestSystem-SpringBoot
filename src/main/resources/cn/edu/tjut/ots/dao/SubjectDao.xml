    <select id="queryBriefSubject" resultMap="subjectListMap">
        select s.uuid,s.subjectName,s.subjectScore,to_char(s.updateWhen,'yyyy-mm-dd') as updateWhenStr,b.name as subjectType FROM subject s
        right join basedata b on s.subjecttype = b.uuid
